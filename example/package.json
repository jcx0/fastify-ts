{
  "name": "example",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "import": {
      "default": "./dist/index.mjs"
    }
  },
  "dependencies": {
    "fastify": "^4.26.2",
    "fastify-openapi-glue": "^4.6.0",
    "fastify-plugin": "^4.5.1",
    "neverthrow": "^7.0.0"
  },
  "devDependencies": {
    "@stylewoven/fastify-ts": "^1.0.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.7",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4",
    "pkgroll": "^2.4.1",
    "prettier": "3.2.5",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "clean": "rimraf dist ./src/gen node_modules/.cache",
    "fastify-ts": "fastify-ts",
    "copy-spec": "cp ./api.yaml ./dist/api.yaml",
    "build": "npm run fastify-ts && pkgroll --minify && npm run copy-spec",
    "start": "node dist/index.mjs",
    "dev": "concurrently \"pkgroll --watch\" \"nodemon dist/index.mjs\""
  }
